<% blog.articles[0...10].each do |article| %>
  <article>
    <h1><a href="<%= article.url %>"><%= article.title %></a></h1>
    <time><%= article.date.strftime('%b %e, %Y') %></time>
    <p><%= article.summary %></p>
    <p><a href="<%= article.url %>">Read more.</a></p>
  </article>
<% end %>
